# first time configuration
git config pull.rebase false
git config user.name 'artur'
git config user.email 'artur.schneider@biologie.uni-freiburg.de'

#initialize current directory as git-repository
git init
#print current status
git status
git add my_file.txt
git status
git commit -m 'added my_file.txt'



# add remote repository as origin
git remote add origin https://github.com/arturoptophys/git_workshop_for5159.git
git fetch

git push --set-upstream origin master
git checkout main
git merge --allow-unrelated-histories master

git some other command

def my_newfunction(input):
	return do_otherstuff(input)


